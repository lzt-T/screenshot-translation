{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*", 
    "src/main/**/*", 
    "src/preload/**/*", 
    "src/type/**/*",
    "src/utils/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
